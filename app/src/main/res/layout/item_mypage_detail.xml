<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_mypage_detail$Root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:weightSum="360"
    android:gravity="center_horizontal">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="253"
        android:orientation="vertical"
        android:weightSum="@string/FRAGMENT_HEIGHT"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="83.3"
            android:weightSum="83.3"
            android:orientation="vertical">
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="31"/>
            <TextView
                android:id="@+id/item_mypage_detail$TV_date"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="52.3"
                android:gravity="center_horizontal|top"
                android:textSize="15sp"/>
        </LinearLayout>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="365"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="365">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="207">

                    <ImageView
                        android:id="@+id/item_mypage_detail$IV_cover"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/default_Image_color" />
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="25dp"
                        android:layout_alignParentRight="true"
                        android:id="@+id/item_mypage_detail$IV_btn"
                        >

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="5dp"
                            android:layout_alignParentRight="true"
                            android:layout_margin="11.3dp"
                            android:background="@drawable/icon_more" />
                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="158"
                    android:weightSum="328">

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="16" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="296"
                        android:gravity="center_vertical"
                        android:weightSum="296"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:weightSum="158"
                            android:orientation="vertical">
                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="11"/>
                            <TextView
                                android:id="@+id/item_mypage_detail$TV_city"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="27.3"
                                android:textSize="14sp"/>
                            <TextView
                                android:id="@+id/item_mypage_detail$TV_address"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="37.6"
                                android:textSize="12sp"/>
                            <TextView
                                android:id="@+id/item_mypage_detail$TV_time"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="20.7"
                                android:textSize="12sp"/>
                            <TextView
                                android:id="@+id/item_mypage_detail$TV_memo"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="32"
                                android:textSize="11sp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="29.3"
                                android:weightSum="253"
                                android:orientation="horizontal">
                                <ImageView
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_weight="166"/>
                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="87"
                                    android:gravity="center_vertical"
                                    android:orientation="vertical"
                                    android:weightSum="40"
                                    >

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:layout_weight="24"
                                        android:gravity="center"
                                        android:weightSum="128">

                                        <ImageView
                                            android:id="@+id/item_mypage_detail$IV_like"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="24"
                                            android:background="@drawable/icon_heart_unclick" />

                                        <TextView
                                            android:id="@+id/item_mypage_detail$TV_like"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/feed_num_color"
                                            android:textSize="@dimen/TextSize_Feed_Num" />

                                        <ImageView
                                            android:layout_width="0dp"
                                            android:layout_height="0dp"
                                            android:layout_weight="7.7" />

                                        <ImageView
                                            android:id="@+id/item_mypage_detail$IV_comment"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="24"
                                            android:background="@drawable/icon_comment" />

                                        <TextView
                                            android:id="@+id/item_mypage_detail$TV_comment"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/feed_num_color"
                                            android:textSize="@dimen/TextSize_Feed_Num" />

                                        <ImageView
                                            android:layout_width="0dp"
                                            android:layout_height="0dp"
                                            android:layout_weight="7.7" />

                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="16" />
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>
</LinearLayout>