<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/item_mypage_detail$Root"
    android:layout_width="253dp"
    android:layout_height="match_parent"
    android:layout_marginLeft="24.3dp"
    android:gravity="center_horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="365">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="207"
            android:background="@color/default_Image_color">

            <ImageView
                android:id="@+id/item_mypage_detail$IV_cover"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <ImageView
                android:id="@+id/item_mypage_detail$IV_more"
                android:layout_width="25dp"
                android:layout_height="5dp"
                android:layout_alignParentRight="true"
                android:layout_margin="10.7dp"
                android:layout_weight="5"
                app:srcCompat="@drawable/icon_more" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="219.6"
            android:orientation="horizontal"
            android:weightSum="253">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="16.7" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="200"
                android:orientation="vertical"
                android:weightSum="158">

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="10.7" />

                <TextView
                    android:id="@+id/item_mypage_detail$TV_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="14" />

                <TextView
                    android:id="@+id/item_mypage_detail$TV_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="10.3" />

                <TextView
                    android:id="@+id/item_mypage_detail$TV_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="8.7" />

                <TextView
                    android:id="@+id/item_mypage_detail$TV_memo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="21.7" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="34.6"
                    android:weightSum="253">

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="149.3" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="103.7"
                        android:orientation="vertical"
                        android:weightSum="34.6">

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="5.3" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="24"
                            android:weightSum="103.7">

                            <ImageView
                                android:id="@+id/item_mypage_detail$IV_like"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="24"
                                app:srcCompat="@drawable/icon_heart_unclick" />

                            <TextView
                                android:id="@+id/item_mypage_detail$TV_like"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textColor="@color/item_mypage_detail_count_color"
                                android:textSize="@dimen/TextSize_Feed_Title" />

                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="7.7" />

                            <ImageView
                                android:id="@+id/item_mypage_detail$IV_comment"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="24"
                                app:srcCompat="@drawable/icon_comment" />

                            <TextView
                                android:id="@+id/item_mypage_detail$TV_comment"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textColor="@color/item_mypage_detail_count_color"
                                android:textSize="@dimen/TextSize_Feed_Title" />
                        </LinearLayout>

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="5.3" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="16.7" />
    </LinearLayout>
</android.support.v7.widget.CardView>